{
  "name": "rpmbuild-action",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript rpmbuild action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "tap",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loganbickmore/rpmbuild.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "loganbickmore",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^7.13.0",
    "eslint-plugin-github": "^4.1.1",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "tap": "^14.11.0",
    "typescript": "^4.0.2"
  }
}
